    },
    {
      "parameters": {
        "language": "python",
        "pythonCode": "isExistingLabel=False\nexistingLabelId=\"\"\nnewLabel=_input.all()[0]['json']['new_label']\nexistingLabel=_input.all()[0]['json']['existing_label']\nexistingLabelsItems=_input.all()[0]['json']['data']\nfor existingLabelItem in existingLabelsItems:\n  if existingLabelItem['name']==newLabel:\n    isExistingLabel=True\n  if existingLabelItem['name']==existingLabel:\n    existingLabelId=existingLabelItem['id']\n  \nreturn [\n  {\n    'json':{\n      \"isExistingLabel\": isExistingLabel,\n      \"newLabel\": newLabel,\n      \"existingLabelId\": existingLabelId\n    }\n  }\n]\n"
      },
