      "parameters": {
        "language": "python",
        "pythonCode": "import ast\n\nexisting_label=\"\"\nnew_label=\"\"\nif len(_input.all()) == 1:\n    item = _input.all()[0]\n    outputItemsStr = item[\"json\"][\"output\"]\n    outputItems = ast.literal_eval(outputItemsStr)\n    if len(outputItems) == 2:\n      new_label = outputItems[0].upper()\n      existing_label = outputItems[1].upper()\n    \n\nreturn [\n        {\n            \"json\": {\n                \"existing_label\": existing_label,\n                \"new_label\": new_label\n            }\n        }\n    ]"
